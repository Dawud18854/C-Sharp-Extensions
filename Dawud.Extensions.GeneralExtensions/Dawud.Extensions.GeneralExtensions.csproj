        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>Dawud.Extensions.GeneralExtensions</PackageId>
        <Version>1.0.0</Version>
